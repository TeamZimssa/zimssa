<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssafy.zimssa.likelist.model.mapper.LikeMapper">



    <insert id="insert" parameterType="com.ssafy.zimssa.likelist.model.dto.Like">
        insert into likelist(price, location, house_name ,user_id)
        values ( #{price},#{location},#{house_name},#{user_id})
    </insert>

    <select id="list" parameterType="String" resultType="com.ssafy.zimssa.likelist.model.dto.Like">
        select price, location, house_name ,user_id from likelist
        where user_id = #{id}
    </select>

    <delete id="delete" parameterType="String">

        delete from likelist where house_name = #{house_name}

    </delete>


</mapper>